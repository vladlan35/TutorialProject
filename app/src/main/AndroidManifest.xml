<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ra.tutorialproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SecondActivity"
            android:label="@string/second_a_name"
            android:parentActivityName=".MainActivity">

        </activity>

        <activity android:name=".ThirdActivity"
            android:label="@string/third_a_name"
            android:parentActivityName=".MainActivity">

        </activity>

        <activity android:name=".FourthActivity"
            android:label="@string/fourth_a_name"
            android:parentActivityName=".MainActivity">

        </activity>

        <activity android:name=".FifthActivity"
            android:label="@string/fifth_a_name"
            android:parentActivityName=".MainActivity">

        </activity>

    <activity android:name=".SixthActivity"
        android:label="@string/sixth_a_name"
        android:parentActivityName=".MainActivity">

    </activity>

    </application>

</manifest>